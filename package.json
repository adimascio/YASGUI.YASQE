  "keywords": [
    "JavaScript",
    "SPARQL",
    "Editor",
    "Semantic Web",
    "Linked Data"
  ],
  "maintainers": [
    {
      "name": "Laurens Rietveld",
      "email": "laurens.rietveld@triply.cc",
      "web": "https://triply.cc"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenTriply/YASGUI.YASQE.git"
  },
  "scripts": {
    "dev": "better-npm-run dev",
    "build": "gulp",
    "upgrade-interactive": "npm-check --skip-unused -u",
    "prettify": "prettier --parser typescript --write $(find ./src -regex '.*\\.tsx?$') && prettier --parser css --write $(find ./src -regex '.*\\.?scss$')"
  },
  "betterScripts": {
    "dev": {
      "command": "webpack-dev-server --port 4000 --config ./webpack/config.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "dependencies": {
    "@types/codemirror": "0.0.53",
    "@types/lodash": "^4.14.91",
    "@types/node": "^8.5.1",
    "codemirror": "^5.32.0",
    "css-loader": "^0.28.7",
    "lodash": "^4.17.4",
    "postcss-loader": "^2.0.9",
    "postcss-scopify": "^0.1.9",
    "prettier": "^1.4.4"
  },
  "devDependencies": {
    "better-npm-run": "^0.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "ts-loader": "^3.2.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-livereload-plugin": "^1.0.0"
  }
}
